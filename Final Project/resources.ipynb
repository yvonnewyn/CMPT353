   "source": [
    "# coal consumption predictions\n",
    "\n",
    "X_train, X_valid, y_train, y_valid = train_test_split(coal_consumption['Year'].values.reshape(-1, 1), coal_consumption['Coal Consumption - TWh'].values.reshape(-1, 1))\n",
    "X_coal_consumption = np.arange(1965, 2021, 1).reshape(-1, 1)\n",
    "\n",
    "poly_coalconsumption = PolynomialFeatures(degree=5)\n",
    "X_poly_coalconsumption = poly_coalconsumption.fit_transform(X_train)\n",
    "\n",
    "model_coalconsumption = LinearRegression(fit_intercept=False)\n",
    "model_coalconsumption.fit(X_poly_coalconsumption, y_train)\n",
    "\n",
    "y_predicted = model_coalconsumption.predict(poly_coalconsumption.fit_transform(X_coal_consumption))\n",
    "\n",
    "plt.plot(coal_consumption['Year'], coal_consumption['Coal Consumption - TWh'], 'b.')\n",
    "plt.plot(X_coal_consumption, y_predicted, 'r-')\n",
    "plt.xlabel(\"Year\")\n",
    "plt.ylabel(\"Coal Consumption (TWh)\")\n",
    "plt.title(\"Coal Consumption 1965-2021\")\n",
    "plt.legend(['Data', 'Model fit'])\n",
    "print(model_coalconsumption.score(poly_coalconsumption.fit_transform(X_train), y_train))\n",
    "print(model_coalconsumption.score(poly_coalconsumption.fit_transform(X_valid), y_valid))\n",
    "\n",
    "y_coalconsumption_predicted = model_coalconsumption.predict(poly_coalconsumption.fit_transform(X_future))\n",
    "y_coalconsumption_predicted2 = model_coalconsumption.predict(poly_coalconsumption.fit_transform(X_nearishfuture))\n",
    "y_coalconsumption_predicted3 = model_coalconsumption.predict(poly_coalconsumption.fit_transform(X_nearfuture))\n",
    "\n"
   ]
  },
