    "\n",
    "gas_production = gas_production[gas_production['Entity']=='World']\n",
    "gas_production = gas_production.drop(columns=['Code'])\n",
    "\n",
    "gas_reserves = gas_reserves[gas_reserves['Entity']=='World']\n",
    "gas_reserves = gas_reserves.drop(columns=['Code'])\n",
    "gas_reserves['TWh'] = gas_reserves['Gas - Proved reserves']*10.55/1000000\n",
    "\n",
    "oil_consumption = oil_consumption[oil_consumption['Entity']=='World']\n",
    "oil_consumption = oil_consumption.drop(columns=['Code'])\n",
    "\n",
    "oil_production = oil_production[oil_production['Entity']=='World']\n",
    "oil_production = oil_production.drop(columns=['Code'])\n",
    "\n",
    "oil_reserves = oil_reserves[oil_reserves['Entity']=='World']\n",
    "oil_reserves = oil_reserves.drop(columns=['Code'])\n",
    "oil_reserves['TWh'] = oil_reserves['Oil - Proved reserves']/85984.52\n",
