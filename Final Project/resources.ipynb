    "co2_emission = co2_emission[co2_emission['Entity']=='World']\n",
    "co2_emission = co2_emission.drop(columns=['Code'])\n",
    "\n",
    "co2_mean = co2_mean.drop(columns=['Unnamed: 3', 'Unnamed: 4'])\n",
    "\n",
    "\n"
   ]
