    "# clean data\n",
    "coal_reserves = coal_reserves[coal_reserves['Country']=='World']\n",
    "coal_reserves = coal_reserves.dropna()\n",
    "coal_reserves['TWh'] = coal_reserves['Total Recoverable Coal (Million Short Tons) 2008']*1000000/122835.03\n",
    "\n",
    "coal_consumption = coal_consumption[coal_consumption['Entity']=='World']\n",
    "coal_consumption = coal_consumption.drop(columns=['Code'])\n",
    "\n",
    "coal_production = coal_production[coal_production['Entity']=='World']\n",
    "coal_production = coal_production.drop(columns=['Code'])\n",
    "\n",
    "gas_consumption = gas_consumption[gas_consumption['Entity']=='World']\n",
